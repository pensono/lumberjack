{
  "name": "lumberjack",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "test": "jest",
    "test-ci": "jest",
    "generate": "nearleyc src/lib/kusto.ne > src/lib/kusto.ts"
  },
  "dependencies": {
    "@mdi/font": "^7.0.96",
    "axios": "^1.2.0",
    "codemirror": "^6.0.1",
    "core-js": "^3.26.1",
    "csv": "^6.2.3",
    "danfojs": "1.1.1",
    "lodash": "^4.17.21",
    "moo": "^0.5.2",
    "nearley": "^2.20.1",
    "re2-wasm": "^1.0.2",
    "splitpanes": "^3.1.5",
    "vue": "^3.2.45",
    "vue-codemirror": "^6.1.1",
    "vuetify": "^3.0.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@esbuild-plugins/node-modules-polyfill": "^0.1.4",
    "@fortawesome/fontawesome-free": "^6.2.1",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jest": "^29.2.3",
    "@types/jsdom": "^20.0.0",
    "@types/lodash": "^4.14.190",
    "@types/nearley": "^2.11.2",
    "@types/node": "^16.11.68",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.4",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.28.0",
    "eslint-plugin-vue": "^9.8.0",
    "jest": "^29.2.1",
    "jest-junit": "^15.0.0",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.0",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vite-plugin-wasm": "^3.1.0",
    "vue-tsc": "^1.0.8"
  },
  "moduleNameMapper": {
    "^@/(.*)": "<rootDir>/src/$1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
